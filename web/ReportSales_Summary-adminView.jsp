  <script>
    var ctx = document.getElementById("chart-bars").getContext("2d");
    var jan = <%=monthlySales[0]%>;
    var feb = <%=monthlySales[1]%>;
    var mar = <%=monthlySales[2]%>;
    var apr = <%=monthlySales[3]%>;
    var may = <%=monthlySales[4]%>;
    var june = <%=monthlySales[5]%>;
    var july = <%=monthlySales[6]%>;
    
    

    
    var top1qty = <%= prodArrList.get(0).getProdPrice() %>;
    var top2qty = <%= prodArrList.get(1).getProdPrice() %>;
    var top3qty = <%= prodArrList.get(2).getProdPrice() %>;
    var top4qty = <%= prodArrList.get(3).getProdPrice() %>;
    var top5qty = <%= prodArrList.get(4).getProdPrice() %>;
    
    
    new Chart(ctx, {
      type: "bar",
      data: {
        labels: ["1", "2", "3", "4", "5"],
        datasets: [{
          label: "Qty Sold",
          tension: 0.4,
          borderWidth: 0,
          borderRadius: 4,
          borderSkipped: false,
          backgroundColor: "rgba(255, 255, 255, .8)",
          data: [top1qty, top2qty, top3qty, top4qty, top5qty],
          maxBarThickness: 6
        }, ],
      },
      options: {
        responsive: true,
        maintainAspectRatio: false,
        plugins: {
          legend: {
            display: false,
          }
        },
        interaction: {
          intersect: false,
          mode: 'index',
        },
        scales: {
          y: {
            grid: {
              drawBorder: false,
              display: true,
              drawOnChartArea: true,
              drawTicks: false,
              borderDash: [5, 5],
              color: 'rgba(255, 255, 255, .2)'
            },
            ticks: {
              suggestedMin: 0,
              suggestedMax: 500,
              beginAtZero: true,
              padding: 10,
              font: {
                size: 14,
                weight: 300,
                family: "Roboto",
                style: 'normal',
                lineHeight: 2
              },
              color: "#fff"
            },
          },
          x: {
            grid: {
              drawBorder: false,
              display: true,
              drawOnChartArea: true,
              drawTicks: false,
              borderDash: [5, 5],
              color: 'rgba(255, 255, 255, .2)'
            },
            ticks: {
              display: true,
              color: '#f8f9fa',
              padding: 10,
              font: {
                size: 14,
                weight: 300,
                family: "Roboto",
                style: 'normal',
                lineHeight: 2
              },
            }
          },
        },
      },
    });


    var ctx2 = document.getElementById("chart-line").getContext("2d");

    new Chart(ctx2, {
      type: "line",
      data: {
        labels: ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul"],
        datasets: [{
          label: "Sales Earning",
          tension: 0,
          borderWidth: 0,
          pointRadius: 5,
          pointBackgroundColor: "rgba(255, 255, 255, .8)",
          pointBorderColor: "transparent",
          borderColor: "rgba(255, 255, 255, .8)",
